{
  "name": "meetingguard-ai",
  "version": "1.0.0",
  "description": "MeetingGuard AI - Smart Meeting Management App",
  "main": "api/index.js",
  "scripts": {
    "start": "node api/index.js",
    "dev": "nodemon api/index.js",
    "expo:start": "expo start",
    "expo:android": "expo start --android",
    "expo:ios": "expo start --ios",
    "expo:web": "expo start --web",
    "build": "expo build",
    "eject": "expo eject",
    "deploy:render": "echo 'Deploy to Render via GitHub integration'",
    "deploy:ios": "eas build --platform ios --profile production",
    "deploy:android": "eas build --platform android --profile production --local",
    "deploy": "npm run deploy:ios && npm run deploy:android",
    "health-check": "curl https://meetingguard-backend.onrender.com/api/health",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.4.1",
    "@sentry/node": "^7.0.0",
    "@sentry/react-native": "~5.20.0",
    "@supabase/supabase-js": "^2.38.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "expo": "~50.0.0",
    "expo-calendar": "~12.2.1",
    "expo-constants": "~15.4.5",
    "expo-dev-client": "~3.3.8",
    "expo-linking": "~6.2.2",
    "expo-notifications": "~0.27.6",
    "expo-router": "~3.4.7",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "expo-web-browser": "~12.8.2",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-web": "~0.19.10",
    "react-native-webview": "13.6.4"
  },
